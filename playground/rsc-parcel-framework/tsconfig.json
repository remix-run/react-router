{
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "./.react-router/types/**/*",
    "./.react-router-parcel/types/**/*"
  ],
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler",
    "module": "esnext",
    "isolatedModules": true,
    "esModuleInterop": true,
    "target": "es2022",
    "noEmit": true,
    "rootDirs": [".", "./.react-router/types", "./.react-router-parcel/types"]
  }
}
