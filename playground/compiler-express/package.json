    "dev:react": "cross-env REACT_SERVER_BUILD=1 node --conditions react-server ./server-react.js",
    "start:prerender": "cross-env NODE_ENV=production node ./server-prerender.js",
    "start:react": "cross-env NODE_ENV=production node --conditions react-server ./server-react.js",
    "start": "pnpm start:react & pnpm start:prerender",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@react-router/express": "workspace:*",
    "@react-router/node": "workspace:*",
    "compression": "^1.7.4",
