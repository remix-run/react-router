  "authors": [
    "Ryan Florence",
    "Michael Jackson"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.0.12",
    "babel-core": "^5.0.12",
    "babel-loader": "^5.0.0",
    "bundle-loader": "^0.5.2",
    "events": "1.0.2",
    "expect": "^1.1.0",
    "jsxhint": "^0.12.1",
    "karma": "^0.12.28",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-mocha": "^0.1.10",
    "karma-sourcemap-loader": "^0.3.2",
    "karma-webpack": "^1.3.1",
    "marked": "0.3.3",
    "mocha": "^2.0.1",
    "pygmentize-bundled": "^2.3.0",
    "react": "0.13.x",
    "rf-changelog": "^0.4.0",
    "rx": "2.3.18",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6"
  },
  "peerDependencies": {
    "react": "0.13.x"
